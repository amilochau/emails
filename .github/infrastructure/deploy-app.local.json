{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "organizationName": {
      "value": "mil"
    },
    "applicationName": {
      "value": "emails"
    },
    "environmentName": {
      "value": "Development"
    },
    "hostName": {
      "value": "local"
    },
    "monitoring": {
      "value": {
        "enableApplicationInsights": false
      }
    },
    "configuration": {
      "value": {
        "enableAppConfiguration": false
      }
    },
    "secrets": {
      "value": {
        "enableKeyVault": false
      }
    },
    "messaging": {
      "value": {
        "enableServiceBus": true,
        "serviceBusQueues": [
          "emails"
        ]
      }
    },
    "storage": {
      "value": {
        "enableStorage": true,
        "storageAccounts": [
          {
            "number": "1",
            "containers": [
              "default"
            ],
            "readOnly": true,
            "daysBeforeDeletion": 30
          }
        ]
      }
    }
  }
}