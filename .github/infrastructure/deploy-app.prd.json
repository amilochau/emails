{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "organizationName": {
      "value": "mil"
    },
    "applicationName": {
      "value": "emails"
    },
    "hostName": {
      "value": "prd"
    },
    "pricingPlan": {
      "value": "Free"
    },
    "applicationType": {
      "value": "isolatedDotnet6"
    },
    "cosmosContainers": {
      "value": [
        {
          "name": "emails",
          "partitionKey": "/id",
          "includedPaths": [],
          "excludedPaths": [
            {
              "path": "/*"
            },
            {
              "path": "/\"_etag\"/?"
            }
          ]
        }
      ]
    },
    "serviceBusOptions": {
      "value": {
        "queues": [
          "emails"
        ],
        "authorizeClients": true
      }
    },
    "storageAccountsOptions": {
      "value": {
        "accounts": [
          {
            "suffix": "1",
            "comment": "attachments",
            "containers": [
              "attachments"
            ],
            "readOnly": true,
            "daysBeforeDeletion": 30,
            "allowBlobPublicAccess": false,
            "authorizeClients": true
          }
        ]
      }
    },
    "extraAppSettings": {
      "value": {
        "SendGrid__Key": "<secret>SendGrid--Key</secret>"
      }
    }
  }
}