{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "organizationName": {
      "value": "mil"
    },
    "applicationName": {
      "value": "emails"
    },
    "environmentName": {
      "value": "Development"
    },
    "hostName": {
      "value": "dev"
    },
    "monitoring": {
      "value": {
        "enableApplicationInsights": true,
        "disableLocalAuth": true,
        "dailyCap": "0.1",
        "workspaceName": "mil-monitoring-dev-ws",
        "workspaceResourceGroup": "mil-monitoring-dev-rg"
      }
    },
    "configuration": {
      "value": {
        "enableAppConfiguration": true,
        "appConfigurationName": "milochau",
        "appConfigurationResourceGroup": "mil-rg-shd"
      }
    },
    "secrets": {
      "value": {
        "enableKeyVault": true
      }
    },
    "messaging": {
      "value": {
        "enableServiceBus": true,
        "serviceBusQueues": [
          "emails"
        ]
      }
    },
    "storage": {
      "value": {
        "enableStorage": true,
        "storageAccounts": [
          {
            "number": "1",
            "containers": [
              "default"
            ],
            "readOnly": true,
            "daysBeforeDeletion": 30
          }
        ]
      }
    }
  }
}